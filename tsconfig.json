{
  "compilerOptions": {
    "outDir": "./dist/",
    "sourceMap": true,
    "noImplicitAny": true,
    "target": "es5",
    "lib": ["dom", "es2017"],
    "module": "commonjs",
    "noUnusedLocals": false,
    "esModuleInterop": true,
    "jsx": "react"
  },
  "include": [
    "./src/**/*",
    "./gatsby-node/**/*"
  ]
}